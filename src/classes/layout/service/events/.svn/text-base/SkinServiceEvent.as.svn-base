package layout.service.events {
	import layout.model.SkinModel;

	import flash.events.Event;

	/**
	 * @author projects
	 */
	public class SkinServiceEvent extends Event {

		public static const LOADED:String = "LOADED";

		public var model:SkinModel;

		public function SkinServiceEvent (type:String, model:SkinModel)
		{
			this.model = model;
			super(type);
		}
	}
}
